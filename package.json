{
  "name": "ag-sistemas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "tailwind:init": "tailwindcss init -p",
    "test": "jest",
    "db:generate": "npx dotenv -e .env -- npx prisma generate",
